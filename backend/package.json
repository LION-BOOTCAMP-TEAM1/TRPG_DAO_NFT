{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env ts-node-dev --respawn src/server.ts",
    "seed": "cross-env ts-node src/seed/seed.ts",
    "db:sync": "npm run db:prisma:generate && ts-node scripts/dbSync.ts --forceMigrate=false --seed=false",
    "db:sync:full": "npm run db:prisma:generate && ts-node scripts/dbSync.ts --forceMigrate=true --seed=true",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:status": "prisma migrate status",
    "start": "cross-env NODE_ENV=production ts-node src/server.ts",
    "db:prisma:generate": "prisma generate",
    "db:init-and-seed": "npm run db:reset && npm run seed",
    "db:fresh": "prisma migrate reset --force && npm run seed",
    "dev:with-seed": "npm run seed && npm run dev",
    "setup-and-dev": "npm run db:prisma:generate && npm run db:migrate && npm run seed && npm run dev"
  },
